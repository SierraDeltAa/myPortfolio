lid #D32F2F;\r\n}\r\n\r\n\r\ninput:focus{\r\n    outline: none;\r\n    border: 2px solid #D32F2F;\r\n}\r\n\r\ninput::placeholder{\r\n    color: #202030;\r\n}\r\n\r\ntextarea{\r\n    background-color: #C4C4C4;\r\n    margin: 25px 0px;\r\n    border-radius: 3px;\r\n    padding: 5px;\r\n    border: 2px solid transparent;\r\n    transition: all 0.3s ease-out;\r\n\r\n}\r\n\r\ntextarea:hover{\r\n    outline: none;\r\n    border: 2px solid #D32F2F;\r\n}\r\n\r\ntextarea:focus{\r\n    outline: none;\r\n    border: 2px solid #D32F2F;\r\n}\r\n\r\ntextarea::placeholder{\r\n    color: #202030;\r\n\r\n}\r\n\r\n/* IMG ANIMATION */ \r\n\r\n.fade-in-down {\r\n    animation: fadeInDown ease-in-out forwards 1s;\r\n    -webkit-animation: fadeInDown ease-in-out forwards 1s;\r\n    -moz-animation: fadeInDown ease-in-out forwards 1s;\r\n    -o-animation: fadeInDown ease-in-out forwards 1s;\r\n    -ms-animation: fadeInDown ease-in-out forwards 1s;\r\n  }\r\n\r\n\r\n  @keyframes fadeInDown {\r\n    from {\r\n        opacity:0;\r\n        padding-bottom: 200px;\r\n    }\r\n    to {\r\n        opacity:1;\r\n        padding-bottom: 0px;\r\n    }\r\n  }\r\n  \r\n  @-moz-keyframes fadeInDown {\r\n    from {\r\n        opacity:0;\r\n        padding-bottom: 200px;\r\n    }\r\n    to {\r\n        opacity:1;\r\n        padding-bottom: 0px;\r\n    }\r\n  }\r\n  \r\n  @-webkit-keyframes fadeInDown {\r\n    from {\r\n        opacity:0;\r\n        padding-bottom: 200px;\r\n    }\r\n    to {\r\n        opacity:1;\r\n        padding-bottom: 0px;\r\n    }\r\n  }\r\n  \r\n  @-o-keyframes fadeInDown {\r\n    from {\r\n        opacity:0;\r\n        padding-bottom: 200px;\r\n    }\r\n    to {\r\n        opacity:1;\r\n        padding-bottom: 0px;\r\n    }\r\n  }\r\n  \r\n  @-ms-keyframes fadeInDown {\r\n    from {\r\n        opacity:0;\r\n        padding-bottom: 200px;\r\n    }\r\n    to {\r\n        opacity:1;\r\n        padding-bottom: 0px;\r\n    }\r\n  }\r\n"],"names":[],"version":3,"file":"index.HASH_REF_1