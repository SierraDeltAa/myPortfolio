    "@parcel/transformer-image": "^2.0.0-beta.2",
    "@parcel/transformer-raw": "^2.0.0-alpha.3",
    "cssnano": "^5.0.2",
    "gh-pages": "^3.1.0",
    "parcel": "^2.0.0-beta.2",
    "postcss-cli": "^8.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "react-burger-menu": "^3.0.6",
    "react-mail-form": "^2.1.0",
    "react-typing-effect": "^2.0.5",
    "tailwindcss": "^2.1.2"
  }
